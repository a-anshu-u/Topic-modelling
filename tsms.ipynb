        "import pandas as pd\n",
        "from sklearn.feature_extraction.text import CountVectorizer\n",
        "from sklearn.decomposition import LatentDirichletAllocation\n",
        "\n",
        "# Assuming the text data is in a column named \"Abstracts\"\n",
        "abstracts = df[\"Abstract\"].tolist()\n",
        "\n",
        "# Create a DataFrame for further processing\n",
        "data = pd.DataFrame({'Abstract': abstracts})\n",
        "\n",
        "# Preprocess the text using CountVectorizer\n",
        "vectorizer = CountVectorizer(stop_words='english')\n",
        "X = vectorizer.fit_transform(data['Abstract'])\n",
        "\n",
        "# Apply Latent Dirichlet Allocation\n",
        "num_topics = 3  # You can adjust the number of topics\n",
        "lda = LatentDirichletAllocation(n_components=num_topics, random_state=42)\n",
        "lda.fit(X)\n",
        "\n",
        "# Create a DataFrame for the top words and their weights for each topic\n",
        "feature_names = vectorizer.get_feature_names_out()\n",
        "topics_df = pd.DataFrame({f'Topic #{i+1}': [feature_names[j] for j in topic.argsort()[-10:]][::-1] for i, topic in enumerate(lda.components_)})\n",
        "\n",
        "# Display the top words for each topic\n",
        "print(\"Top Words for Each Topic:\")\n",
        "print(topics_df)\n",
        "\n",
        "# Transform the abstracts into topic space\n",
        "topic_results = lda.transform(X)\n",
        "data['Dominant_Topic'] = topic_results.argmax(axis=1) + 1\n",
        "\n",
        "# Display the abstracts with their dominant topics\n",
        "print(\"\\nAbstracts with Dominant Topics:\")\n",
        "print(data[['Abstract', 'Dominant_Topic']])\n",
        "\n",
        "# Rank topics based on their average scores\n",
        "topic_ranking = topic_results.mean(axis=0).argsort()[::-1] + 1\n",
        "print(\"\\nTopic Ranking:\")\n",
        "print(topic_ranking)\n"
      ]
    },
